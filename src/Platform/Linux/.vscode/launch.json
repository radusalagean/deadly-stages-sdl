{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Linux Launch",
            "type": "cppdbg",
            "program": "${workspaceFolder:Linux}/../../../build/linux-x86_64-debug/DeadlyStagesRemix",
            "request": "launch",
            "cwd": "${workspaceFolder:Linux}/../../../build/linux-x86_64-debug",
